{
  "expo": {
    "name": "WorldUniversities",
    "slug": "WorldUniversities",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.princewilliroka.worlduniversitiesapp"
    },
    "android": {
      "package": "com.princewilliroka.worlduniversitiesapp",
      "permissions": ["WAKE_LOCK"],
      "versionCode": 5
    },
    "androidStatusBar": {
      "backgroundColor": "#1a1a1a"
    }
  }
}